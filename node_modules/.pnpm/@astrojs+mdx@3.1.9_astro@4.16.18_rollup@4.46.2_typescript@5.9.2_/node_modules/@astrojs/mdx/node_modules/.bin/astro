#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/azureuser/withlia-2/node_modules/.pnpm/astro@4.16.18_rollup@4.46.2_typescript@5.9.2/node_modules/astro/node_modules:/home/azureuser/withlia-2/node_modules/.pnpm/astro@4.16.18_rollup@4.46.2_typescript@5.9.2/node_modules:/home/azureuser/withlia-2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/azureuser/withlia-2/node_modules/.pnpm/astro@4.16.18_rollup@4.46.2_typescript@5.9.2/node_modules/astro/node_modules:/home/azureuser/withlia-2/node_modules/.pnpm/astro@4.16.18_rollup@4.46.2_typescript@5.9.2/node_modules:/home/azureuser/withlia-2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../astro@4.16.18_rollup@4.46.2_typescript@5.9.2/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../../astro@4.16.18_rollup@4.46.2_typescript@5.9.2/node_modules/astro/astro.js" "$@"
fi
